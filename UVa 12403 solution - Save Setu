#include <stdio.h>
#include <string.h>
int main() 
{
    int T, K,sum = 0;
    char str[20];
    scanf("%d", &T);
    while(T--) 
    {
        scanf("%s", str);
        if(!strcmp(str, "donate")) 
          {
            scanf("%d", &K);
            sum += K;
          } 
           else 
             {
               printf("%d\n", sum);
             }
    }
    return 0;
}
